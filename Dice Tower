#include <bits/stdc++.h>
#define fastIO ios::sync_with_stdio(false); cin.tie(0)
using namespace std;


int main() {
    fastIO;
    int t;
    cin>>t;
    while(t--)
    {
    	    long long n;
    	    cin>>n;
    	    
    	    long long d = n/14;
        	long long t = n%14;
        	
        	if(d>=1 && t>=1 && t<=6)
        	cout<<"YES"<<endl;
        	else
        	cout<<"NO"<<endl;
    }
    return 0;
}
