#include <bits/stdc++.h>
#define fastIO ios::sync_with_stdio(false); cin.tie(0)
using namespace std;

void solve()
{
    int n;
    cin>>n;
    
    long long x = log2(n-1);
    x = pow(2,x);
    
    for(int i=x-1; i>=0; i--)   cout<<i<<" ";
    for(int i=x; i<n; i++)      cout<<i<<" ";
    
    cout<<endl;
}

int main() {
    fastIO;
    int t;
    cin>>t;
    // solve();
    while(t--)
    {
        solve();
    }
    return 0;
}
