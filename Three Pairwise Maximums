#include <bits/stdc++.h>
#define fastIO ios::sync_with_stdio(false); cin.tie(0)
using namespace std;

int main() {
    fastIO;
    int t;
    cin>>t;
    while(t--)
    {
        vector<int> arr(3);
        for(int i=0; i<3; i++)
        {
            cin>>arr[i];
        }
        sort(arr.begin(),arr.end());
        if(arr[1]!=arr[2])
        cout<<"NO"<<endl;
        else
        {
            cout<<arr[0]<<" "<<arr[0]<<" "<<arr[2]<<endl;
        }
    }
    return 0;
}
