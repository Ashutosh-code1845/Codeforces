#include <bits/stdc++.h>
#define fastIO ios::sync_with_stdio(false); cin.tie(0)
using namespace std;

void solve()
{
    string s="";
    cin>>s;
    
    if(s.size()==0)
    {
        cout<<0<<endl;
        return;
    }
    
    unordered_map<char,int> mp;
    
    int n = s.size();
    mp[s[0]]=0;
    vector<int> ans(n,0);
    
    for(int i=1; i<s.size(); i++)
    {
        if(mp.find(s[i])!=mp.end())
        {
            ans[i]=2+ans[mp[s[i]]-1];
        }
        
        ans[i]=max(ans[i],ans[i-1]);
        mp[s[i]]=i;
    }
    cout<<s.size()-ans[n-1]<<endl;
}
int main() {
    fastIO;
    int t;
    cin>>t;
    while(t--)
    {
        solve();
    }
    return 0;
}
