#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        
        long long int ans=0;
        long long int x;
        for(int i=0; i<n; i++)
        {
            cin>>x;
            ans|=x;
        }
        cout<<ans<<endl;
    }
    return 0;
}
