#include <bits/stdc++.h>
#define fastIO ios::sync_with_stdio(false); cin.tie(0)
using namespace std;

int main() {
    fastIO;
    int t;
    cin>>t;
    while(t--)
    {
        int a,b;
        cin>>a>>b;
        
        if(2*a > b)
        cout<<-1<<" "<<-1<<endl;
        else
        {
            cout<<a<<" "<<2*a<<endl;
        }
    }
    return 0;
}
