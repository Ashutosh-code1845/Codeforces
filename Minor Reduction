#include <bits/stdc++.h>
#define fastIO ios::sync_with_stdio(false); cin.tie(0)
using namespace std;

void solve()
{
    string s;
    cin>>s;
    
    int m = s.size();
    int max = (s[1]-'0')+(s[0]-'0');
    int ind = 0;
    int num;
    
    for(int i=0; i<m-1; i++)
    {
        num=(s[i]-'0')+(s[i+1]-'0');
        if(num>=10)
        {
            max=num;
            ind = i;
        }
    }
    for(int i=0; i<m; i++)
    {
        if(i==ind)
        {
            cout<<max;
            i++;
            continue;
        }
        cout<<s[i];
    }
    cout<<endl;
}
int main() {
    fastIO;
    int t;
    cin>>t;
    // solve();
    while(t--)
    {
        solve();
    }
    return 0;
}
