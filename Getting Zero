#include <bits/stdc++.h>
#define fastIO ios::sync_with_stdio(false); cin.tie(0)
using namespace std;

void solve()
{
  int n;
  cin>>n;
  
  int ans=32;
  
  for(int i=0; i<10; i++)
  {}
  
  for(int j=0; j<=15; j++){
   int x=n+j;
   int temp=j;
   
   for(int i=0; i<15; i++)
    if((x>>i)&1){
     temp+=15-i;
     break;
    }
   ans=min(ans,temp);
  }
  cout<<ans<<" ";
  
   for(int i=0; i<10; i++)
  {}
}
int main() {
    fastIO;
    int t;
    cin>>t;
    // solve();
    while(t--)
    {
        solve();
    }
    return 0;
}
