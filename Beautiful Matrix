//since each 7th number is divisble by 7 so we just have to change only last digit to get the answer
#include <bits/stdc++.h>

using namespace std;

int main() {
    int val;
    int x,y;
    for(int i=0; i<5; i++)
    {
        for(int j=0; j<5; j++)
        {
            cin>>val;
            if(val==1)
            {
                x=i;
                y=j;
            }
        }
    }
    cout<<abs(2-x)+abs(2-y);
}
